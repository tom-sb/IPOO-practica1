#include<stdio.h>

int index(char *,char t);
char t='a';

int main(){
	char palabra[20];

	printf("escribe una palabra: ");
	gets(palabra);


	printf("\n %i veces",index(palabra,t));

	return 0;
}


int index(char *s,char t){

    int i=0;
    for (i=0;i<=20;i++){
        if (*s[i]==t) return i;
    }
}
